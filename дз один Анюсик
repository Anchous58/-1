print ("введите длину длинного бортика")
n= int(input())
print ("введите длину короткого бортика")
m= int(input())
print ("введите известное расстояние человека до длинного бортика")
x= int(input())
print ("введите известное расстояние человека до короткого бортика")
y= int(input())

a = n - y
b = m - x

if x >= b
  print (b)
  print ("- минимальное расстояние до длинного бортика")
else:
    print (x)
    print ("- минимальное расстояние до длинного бортика")

if y >= a:
    print (a)
    print ("- минимальное расстояние до короткого бортика")
else:
    print (y)
    print ("- минимальное расстояние до короткого бортика")

    
if a <= b:
    print(a)
    print("- СААААмое минимальное расстояние до бортика")
else:
    print(b)
    print("- СААААмое минимальное расстояние до бортика")
